{
  "name": "switchTabs",
  "version": "1.0.0",
  "description": "Build an Extension!",
  "manifest_version": 3,
  "action": {
    "default_popup": "mainPage/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content-script.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "channel1": {
      "suggested_key": {
        "default": "Alt+Shift+1",
        "mac": "Command+Shift+1"
      },
      "description": "channel1"
    },
    "next-ch": {
      "suggested_key": {
        "default": "Alt+Shift+2",
        "mac": "Command+Shift+2"
      },
      "description": "next"
    },
    "previous-ch": {
      "suggested_key": {
        "default": "Alt+Shift+3",
        "mac": "Command+Shift+3"
      },
      "description": "previous"
    },
    "volume-up": {
      "suggested_key": {
        "default": "Alt+PageUp",
        "mac": "Command+PageUp"
      },
      "description": "volume up"
    }
  },
  "permissions": ["tabs", "scripting", "nativeMessaging"],
  "host_permissions": [
    "http://example.com/",
    "<all_urls>"
  ]

}
